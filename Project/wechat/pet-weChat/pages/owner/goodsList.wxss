/* pages/owner/goodsList.wxss */
.eee{
  background-color: #eeeeee;
  line-height: 0;
  --card-thumb-size:90px;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  /* --card-background-color:#fff; */
}

.van-card{
  padding: 15px!important;
  width: 750rpx!important;
  /* background-color: #fff!important; */
  height:120px;
  overflow: hidden;
  margin:5px 0 0 0 ;
}
.card .card-title{
  font-size: 16px;
  line-height: 23px;
  font-weight: 400;
}
.card .desc{
  font-size: 12px;
  line-height: 16px;
  color: #999;
}
.block{
  display: block;
}
.slot-after{
  height: 138rpx;
}
.no-data{
  position: absolute;
  width: 100%;
  height: 100%;
}
.no-data-image{
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translateX(-50%);
  width: 407rpx;
  height: 270rpx;
}
.right{
  float: right;
}