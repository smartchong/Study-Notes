/* pages/user/agreement.wxss */
.agreement-page{
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #eeeeee;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.area{
  margin: 5px 0;
  background-color: #fff;
  padding: 15px;
  margin-bottom: 5px;
}
.h1{
  display: block;
  height: 40px;
  line-height: 40px;
  font-size: 20px; 
  text-align: center;
}
.h2{
  display: block;
  height: 30px;
  line-height: 30px;
  font-size: 16px; 
  text-align: center;
}
.text{
  display: block;
  font-size: 12px; 
  line-height: 20px;
}
.blod{
  font-weight: 600;
}